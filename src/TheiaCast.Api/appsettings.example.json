{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Jwt": {
    "Issuer": "theiacast",
    "Audience": "theiacast-clients",
    "Secret": "YOUR_SECRET_KEY_HERE_MIN_32_CHARS"
  },
  "License": {
    "Secret": "YOUR_HMAC_SECRET_HERE_MIN_64_CHARS_CONTACT_VENDOR_FOR_LICENSES",
    "EnableValidation": true,
    "GracePeriodDays": 7
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=theiacast;Username=postgres;Password=YOUR_PASSWORD_HERE"
  }
}
